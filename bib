@ARTICLE{Morelli2021-tu,
  title    = "Effects of urbanization on taxonomic, functional and phylogenetic
              avian diversity in Europe",
  author   = "Morelli, Federico and Benedetti, Yanina and
              Ib{\'a}{\~n}ez-{\'A}lamo, Juan Diego and Tryjanowski, Piotr and
              Jokim{\"a}ki, Jukka and Kaisanlahti-Jokim{\"a}ki, Marja-Liisa and
              Suhonen, Jukka and D{\'\i}az, Mario and M{\o}ller, Anders Pape
