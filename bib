@ARTICLE{Arnqvist2020-rx,
  title    = "Mixed Models Offer No Freedom from Degrees of Freedom",
  author   = "Arnqvist, G{\"o}ran",
  abstract = "Statistics matter greatly in biology, whether we like it or not.
              As a discipline with an empirical inclination, we are faced with
              data every day and we rely on inferential statistical models to
              make sense of it and to provide us with novel insights. Much of
              the time, the growing level of complexity and sophistication of
              the models we put to use in ecology and evolution have led to
              more appropriate analyses of our data. However, this is not
              always the case. Here, I draw attention to a classic flaw in
              inferential statistics that has resurfaced in a new flavor as a
              result of increased reliance on complex linear mixed models - the
              multifaceted and disturbingly persistent problem of
              pseudoreplication.",
  journal  = "Trends Ecol. Evol.",
  volume   =  35,
  number   =  4,
  pages    = "329--335",
  month    =  apr,
  year     =  2020,
  keywords = "biostatistics; experimental evolution; hierarchical data; linear
              mixed models; pseudoreplication; type I errors;Dissertation",
  language = "en"
}

